<?php

/**
 * @file
 * Contains showtext.module.
 */
function drupalup_simple_form_entity_view(array &$build, \Drupal\Core\Entity\EntityInterface $entity, \Drupal\Core\Entity\Display\EntityViewDisplayInterface $display, $view_mode) {
  if($entity->getEntityType()->id()==='node'){
    $build['awesome']=[
      '#markup'=>'<h2>This is awesome</h2>',
      '#allowed_tags'=>['h2'],
    ];
  }
}